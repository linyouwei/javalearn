package org.uclbrt.util;
/**
 * 系统常量接口
 * @author Administrator
 *
 */
public interface SystemConstant {
	//成功进入edit.html
	int SUCCESS = 0;
	//名字错误
	int NAME_ERROR = 1;
	//密码错误
	int PASSWORD_ERROR = 2;
	//修改密码页面的密码错误
	int ORIGINAL_PASSWORD_ERROR = 3;
	//成功进行修改密码
	int CHANGESUCCESS = 4;
	
	//搜索显示页面的行数
	int PAGE_SIZE = 5;

	
}
